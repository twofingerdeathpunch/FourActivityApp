<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditProfile">

    <FrameLayout
        android:id="@+id/favesLayoutTopLeftLand"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/favesLayoutTopRightLand"
        app:layout_constraintBottom_toTopOf="@+id/favesLayoutBottomLeftLand"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/favesLayoutTopRightLand"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/favesLayoutTopLeftLand"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/favesLayoutBottomRightLand"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/favesLayoutBottomLeftLand"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/favesLayoutBottomRightLand"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favesLayoutTopLeftLand" />

    <FrameLayout
        android:id="@+id/favesLayoutBottomRightLand"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/favesLayoutBottomLeftLand"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favesLayoutTopRightLand" >

    </FrameLayout>
</android.support.constraint.ConstraintLayout>
